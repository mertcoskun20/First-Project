Coefficient Correlation calories-lightly active minutes = var _muX = Calculate(average(average_summations[Average of Calories]))
var _muY = CALCULATE(average(average_summations[Average of Lighty Active Minutes]))
var _numerator = SUMX(average_summations,(average_summations[Average of Calories] - _muX)* (average_summations[Average of Lighty Active Minutes] - _muY))
var _denominator = sqrt(sumx(average_summations, (average_summations[Average of Calories] - _muX)^2)* Sumx(average_summations, (average_summations[Average of Lighty Active Minutes] - _muY)^2))

return

divide(_numerator, _denominator)

